local function checkMirage()
    for _, island in pairs(game.Workspace:GetChildren()) do
        if string.find(island.Name, "Mirage") then
            return true -- Mirage tồn tại
        end
    end
    return false -- Mirage không tồn tại
end

-- Kiểm tra Mirage
if checkMirage() then
    print("🎯 Mirage Island Đã Xuất Hiện! Chạy script AB...")
    
    -- 📌 Chạy Script A (Thay thế bằng code của bạn)
    loadstring(game:HttpGet("bỏ script gạt vô", true))()

else
    print("❌ Mirage Không Xuất Hiện! Chạy script CD...")

    -- 📌 Chạy Script B (Thay thế bằng code của bạn)
    loadstring(game:HttpGet("bỏ script webhook vô", true))()
end


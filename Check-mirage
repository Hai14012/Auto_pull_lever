local function checkMirage()
    for _, island in pairs(game.Workspace:GetChildren()) do
        if string.find(island.Name, "Mirage") then
            return true -- Mirage tá»“n táº¡i
        end
    end
    return false -- Mirage khÃ´ng tá»“n táº¡i
end

-- Kiá»ƒm tra Mirage
if checkMirage() then
    print("ğŸ¯ Mirage Island ÄÃ£ Xuáº¥t Hiá»‡n! Cháº¡y script AB...")
    
    -- ğŸ“Œ Cháº¡y Script A (Thay tháº¿ báº±ng code cá»§a báº¡n)
    loadstring(game:HttpGet("bá» script gáº¡t vÃ´", true))()

else
    print("âŒ Mirage KhÃ´ng Xuáº¥t Hiá»‡n! Cháº¡y script CD...")

    -- ğŸ“Œ Cháº¡y Script B (Thay tháº¿ báº±ng code cá»§a báº¡n)
    loadstring(game:HttpGet("bá» script webhook vÃ´", true))()
end

